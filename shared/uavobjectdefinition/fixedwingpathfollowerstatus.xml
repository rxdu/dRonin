<xml>
  <object name="FixedWingPathFollowerStatus" settings="false" singleinstance="true">
    <description>Object Storing Debugging Information on PID internals</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="50000"/>
    <field defaultvalue="0" name="Error" type="float" units="">
      <description/>
      <elementnames>
        <elementname>Bearing</elementname>
        <elementname>Speed</elementname>
        <elementname>Accel</elementname>
        <elementname>Power</elementname>
      </elementnames>
    </field>
    <field defaultvalue="0" name="ErrorInt" type="float" units="">
      <description/>
      <elementnames>
        <elementname>Bearing</elementname>
        <elementname>Speed</elementname>
        <elementname>Accel</elementname>
        <elementname>Power</elementname>
      </elementnames>
    </field>
    <field defaultvalue="0" name="Command" type="float" units="">
      <description/>
      <elementnames>
        <elementname>Bearing</elementname>
        <elementname>Speed</elementname>
        <elementname>Accel</elementname>
        <elementname>Power</elementname>
      </elementnames>
    </field>
    <field defaultvalue="0" name="Errors" type="uint8" units="">
      <description/>
      <elementnames>
        <elementname>Wind</elementname>
        <elementname>Stallspeed</elementname>
        <elementname>Lowspeed</elementname>
        <elementname>Highspeed</elementname>
        <elementname>Overspeed</elementname>
        <elementname>Lowpower</elementname>
        <elementname>Highpower</elementname>
        <elementname>Pitchcontrol</elementname>
      </elementnames>
    </field>
  </object>
</xml>
