<xml>
  <object name="GPSTime" settings="false" singleinstance="true">
    <description>Contains the GPS time from @ref GPSModule.  Required to compute the world magnetic model correctly when setting the home location.</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="periodic" period="30000"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="10000"/>
    <field defaultvalue="0" elements="1" name="Month" type="int8" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Day" type="int8" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Year" type="int16" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Hour" type="int8" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Minute" type="int8" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Second" type="int8" units="">
      <description/>
    </field>
  </object>
</xml>
