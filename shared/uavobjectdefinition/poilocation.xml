<xml>
  <object name="PoiLocation" settings="false" singleinstance="true">
    <description>Contains the current Point of interest relative to @ref HomeLocation</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="periodic" period="1000"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="1000"/>
    <field defaultvalue="0" elements="1" name="North" type="float" units="m">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="East" type="float" units="m">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Down" type="float" units="m">
      <description/>
    </field>
  </object>
</xml>
