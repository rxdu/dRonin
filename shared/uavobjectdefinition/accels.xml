<xml>
  <object name="Accels" settings="false" singleinstance="true">
    <description>The accelerometer sensor data, rotated into body frame.</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="1000"/>
    <field defaultvalue="0" elements="1" name="x" type="float" units="m/s^2">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="y" type="float" units="m/s^2">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="z" type="float" units="m/s^2">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="temperature" type="float" units="deg C">
      <description/>
    </field>
  </object>
</xml>
