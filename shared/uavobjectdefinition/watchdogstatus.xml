<xml>
  <object name="WatchdogStatus" settings="false" singleinstance="true">
    <description>For monitoring the flags in the watchdog and especially the bootup flags</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="periodic" period="5000"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="5000"/>
    <field defaultvalue="0" elements="1" name="BootupFlags" type="uint16" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="ActiveFlags" type="uint16" units="">
      <description/>
    </field>
  </object>
</xml>
