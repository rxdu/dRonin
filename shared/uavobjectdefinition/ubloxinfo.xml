<xml>
  <object name="UBloxInfo" settings="false" singleinstance="true">
    <description>UBlox specific information</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="periodic" period="1000"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="periodic" period="10000"/>
    <field defaultvalue="0" elements="1" name="swVersion" type="uint32" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="hwVersion" type="uint16" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="ParseErrors" type="uint32" units="">
      <description/>
    </field>
  </object>
</xml>
