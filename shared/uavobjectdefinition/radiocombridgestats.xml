<xml>
  <object name="RadioComBridgeStats" settings="false" singleinstance="true">
    <description>Maintains the telemetry statistics from the OPLM RadioComBridge.</description>
    <access gcs="readonly" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="5000"/>
    <field defaultvalue="0" elements="1" name="TelemetryTxBytes" type="uint32" units="bytes">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="TelemetryTxFailures" type="uint32" units="count">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="TelemetryTxRetries" type="uint32" units="count">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="TelemetryRxBytes" type="uint32" units="bytes">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="TelemetryRxFailures" type="uint32" units="count">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="TelemetryRxSyncErrors" type="uint32" units="count">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="TelemetryRxCrcErrors" type="uint32" units="count">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="RadioTxBytes" type="uint32" units="bytes">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="RadioTxFailures" type="uint32" units="count">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="RadioTxRetries" type="uint32" units="count">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="RadioRxBytes" type="uint32" units="bytes">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="RadioRxFailures" type="uint32" units="count">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="RadioRxSyncErrors" type="uint32" units="count">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="RadioRxCrcErrors" type="uint32" units="count">
      <description/>
    </field>
  </object>
</xml>
