<xml>
  <object name="AltitudeHoldDesired" settings="false" singleinstance="true">
    <description>Holds the desired altitude (from manual control) as well as the desired attitude to pass through</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="periodic" period="1000"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="onchange" period="0"/>
    <field defaultvalue="0" elements="1" name="Altitude" type="float" units="m">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="ClimbRate" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Roll" type="float" units="deg">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Pitch" type="float" units="deg">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Yaw" type="float" units="deg/s">
      <description/>
    </field>
  </object>
</xml>
