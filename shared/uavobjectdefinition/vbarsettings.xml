<xml>
  <object name="VbarSettings" settings="true" singleinstance="true">
    <description>Settings for the virtualbar flight mode</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="true" updatemode="onchange" period="0"/>
    <telemetryflight acked="true" updatemode="onchange" period="0"/>
    <field defaultvalue="0.5" name="VbarSensitivity" type="float" units="frac">
      <description/>
      <elementnames>
        <elementname>Roll</elementname>
        <elementname>Pitch</elementname>
        <elementname>Yaw</elementname>
      </elementnames>
    </field>
    <field defaultvalue="0.005,0.002,0.0" name="VbarRollPID" type="float" units="1/(deg/s)">
      <description/>
      <elementnames>
        <elementname>Kp</elementname>
        <elementname>Ki</elementname>
        <elementname>Kd</elementname>
      </elementnames>
    </field>
    <field defaultvalue="0.005,0.002,0.0" name="VbarPitchPID" type="float" units="1/(deg/s)">
      <description/>
      <elementnames>
        <elementname>Kp</elementname>
        <elementname>Ki</elementname>
        <elementname>Kd</elementname>
      </elementnames>
    </field>
    <field defaultvalue="0.005,0.002,0.0" name="VbarYawPID" type="float" units="1/(deg/s)">
      <description/>
      <elementnames>
        <elementname>Kp</elementname>
        <elementname>Ki</elementname>
        <elementname>Kd</elementname>
      </elementnames>
    </field>
    <field defaultvalue="0.5" elements="1" name="VbarTau" type="float" units="sec">
      <description/>
    </field>
    <field defaultvalue="30" elements="1" name="VbarGyroSuppress" type="int8" units="%">
      <description/>
    </field>
    <field defaultvalue="FALSE" elements="1" name="VbarPiroComp" type="enum" units="">
      <description/>
      <options>
        <option>FALSE</option>
        <option>TRUE</option>
      </options>
    </field>
    <field defaultvalue="10" elements="1" name="VbarMaxAngle" type="uint8" units="deg">
      <description/>
    </field>
  </object>
</xml>
