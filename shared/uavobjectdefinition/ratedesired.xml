<xml>
  <object name="RateDesired" settings="false" singleinstance="true">
    <description>Status for the matrix mixer showing the output of each mixer after all scaling</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="periodic" period="1000"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="1000"/>
    <field defaultvalue="0" elements="1" name="Roll" type="float" units="deg/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Pitch" type="float" units="deg/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Yaw" type="float" units="deg/s">
      <description/>
    </field>
  </object>
</xml>
