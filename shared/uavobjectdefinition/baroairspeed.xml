<xml>
  <object name="BaroAirspeed" settings="false" singleinstance="true">
    <description>The raw data from the dynamic pressure sensor with pressure, temperature and airspeed.</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="1000"/>
    <field defaultvalue="False" elements="1" name="BaroConnected" type="enum" units="">
      <description/>
      <options>
        <option>False</option>
        <option>True</option>
      </options>
    </field>
    <field defaultvalue="0" elements="1" name="SensorValue" type="uint16" units="raw">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="CalibratedAirspeed" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="GPSAirspeed" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="TrueAirspeed" type="float" units="m/s">
      <description/>
    </field>
  </object>
</xml>
