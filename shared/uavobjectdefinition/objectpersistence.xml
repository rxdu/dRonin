<xml>
  <object name="ObjectPersistence" settings="false" singleinstance="true">
    <description>Someone who knows please enter this</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="true" updatemode="manual" period="0"/>
    <telemetryflight acked="true" updatemode="onchange" period="0"/>
    <field defaultvalue="NOP" elements="1" name="Operation" type="enum" units="">
      <description/>
      <options>
        <option>NOP</option>
        <option>Load</option>
        <option>Save</option>
        <option>Delete</option>
        <option>FullErase</option>
        <option>Completed</option>
        <option>Error</option>
      </options>
    </field>
    <field defaultvalue="0" elements="1" name="ObjectID" type="uint32" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="InstanceID" type="uint32" units="">
      <description/>
    </field>
  </object>
</xml>
