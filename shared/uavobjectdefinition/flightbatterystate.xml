<xml>
  <object name="FlightBatteryState" settings="false" singleinstance="true">
    <description>Battery status information.</description>
    <access gcs="readonly" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="1000"/>
    <field defaultvalue="0.0" elements="1" name="Voltage" type="float" units="V">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="DetectedCellCount" type="uint8" units="">
      <description/>
    </field>
    <field defaultvalue="0.0" elements="1" name="Current" type="float" units="A">
      <description/>
    </field>
    <field defaultvalue="0.0" elements="1" name="BoardSupplyVoltage" type="float" units="V">
      <description/>
    </field>
    <field defaultvalue="0.0" elements="1" name="PeakCurrent" type="float" units="A">
      <description/>
    </field>
    <field defaultvalue="0.0" elements="1" name="AvgCurrent" type="float" units="A">
      <description/>
    </field>
    <field defaultvalue="0.0" elements="1" name="ConsumedEnergy" type="float" units="mAh">
      <description/>
    </field>
    <field defaultvalue="0.0" elements="1" name="EstimatedFlightTime" type="float" units="sec">
      <description/>
    </field>
  </object>
</xml>
