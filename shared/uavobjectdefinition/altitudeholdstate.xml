<xml>
  <object name="AltitudeHoldState" settings="false" singleinstance="true">
    <description>Debugging information for the altitude hold module</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="periodic" period="1000"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="0"/>
    <field defaultvalue="0" elements="1" name="VelocityDesired" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Integral" type="float" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="AngleGain" type="float" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Thrust" type="float" units="">
      <description/>
    </field>
  </object>
</xml>
