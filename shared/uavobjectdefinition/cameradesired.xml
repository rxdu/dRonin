<xml>
  <object name="CameraDesired" settings="false" singleinstance="true">
    <description>Desired camera outputs.  Comes from @ref CameraStabilization module.</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="1000"/>
    <field defaultvalue="0" elements="1" name="Roll" type="float" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Pitch" type="float" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Yaw" type="float" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Bearing" type="float" units="deg">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Declination" type="float" units="deg">
      <description/>
    </field>
  </object>
</xml>
