<xml>
  <object name="GroundTruth" settings="false" singleinstance="true">
    <description>Ground truth data output by a simulator.</description>
    <access gcs="readwrite" flight="readonly"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="false" updatemode="periodic" period="50000"/>
    <telemetryflight acked="false" updatemode="manual" period="50000"/>
    <field defaultvalue="0" elements="3" name="AccelerationXYZ" type="float" units="m/s^2">
      <description/>
    </field>
    <field defaultvalue="0" elements="3" name="PositionNED" type="float" units="m">
      <description/>
    </field>
    <field defaultvalue="0" elements="3" name="VelocityNED" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="3" name="RPY" type="float" units="deg">
      <description/>
    </field>
    <field defaultvalue="0" elements="3" name="AngularRates" type="float" units="deg/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="TrueAirspeed" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="CalibratedAirspeed" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="AngleOfAttack" type="float" units="deg">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="AngleOfSlip" type="float" units="deg">
      <description/>
    </field>
  </object>
</xml>
