<xml>
  <object name="OpticalFlow" settings="false" singleinstance="true">
    <description>The optical flow sensor data, rotated into body frame.</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="periodic" period="100"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="1000"/>
    <field defaultvalue="0" elements="1" name="x" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="y" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="z" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Quality" type="uint8" units="">
      <description/>
    </field>
  </object>
</xml>
