<xml>
  <object name="SubTrim" settings="false" singleinstance="true">
    <description>The trim angle required for the UAV to fly straight and level.</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="1000"/>
    <field defaultvalue="0" elements="1" name="Roll" type="float" units="degrees">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Pitch" type="float" units="degrees">
      <description/>
    </field>
  </object>
</xml>
