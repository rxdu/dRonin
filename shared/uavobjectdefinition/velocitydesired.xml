<xml>
  <object name="VelocityDesired" settings="false" singleinstance="true">
    <description>Used within @ref GuidanceModule to communicate between the task computing the desired velocity and the PID loop to achieve it (running at different rates).</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="periodic" period="1000"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="10000"/>
    <field defaultvalue="0" elements="1" name="North" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="East" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Down" type="float" units="m/s">
      <description/>
    </field>
  </object>
</xml>
