<xml>
  <object name="SessionManaging" settings="false" singleinstance="true">
    <description>Provides session managing to uavtalk</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="true" updatemode="manual" period="0"/>
    <telemetryflight acked="true" updatemode="onchange" period="0"/>
    <field defaultvalue="0" elements="1" name="SessionID" type="uint16" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="ObjectID" type="uint32" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="ObjectInstances" type="uint8" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="NumberOfObjects" type="uint8" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="ObjectOfInterestIndex" type="uint8" units="">
      <description/>
    </field>
  </object>
</xml>
