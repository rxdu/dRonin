<xml>
  <object name="OpenLRSStatus" settings="false" singleinstance="true">
    <description>OpenLRS link status.</description>
    <access gcs="readonly" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="500"/>
    <field defaultvalue="0" elements="1" name="LastRSSI" type="uint8" units="">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="LinkQuality" type="uint16" units="">
      <description/>
    </field>
    <field defaultvalue="Inactive" elements="1" name="FailsafeActive" type="enum" units="function">
      <description/>
      <options>
        <option>Inactive</option>
        <option>Active</option>
      </options>
    </field>
  </object>
</xml>
