<xml>
  <object name="FlightStats" settings="false" singleinstance="true">
    <description>Statistics of the current flight</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="periodic" period="5000"/>
    <field defaultvalue="0" elements="1" name="DistanceTravelled" type="float" units="m">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="MaxDistanceToHome" type="float" units="m">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="MaxAltitude" type="uint16" units="m">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="MaxClimbRate" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="MaxDescentRate" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="MaxGroundSpeed" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="MaxAirSpeed" type="float" units="m/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="MaxRollRate" type="uint16" units="deg/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="MaxPitchRate" type="uint16" units="deg/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="MaxYawRate" type="uint16" units="deg/s">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="ConsumedEnergy" type="uint16" units="mAh">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="InitialBatteryVoltage" type="uint16" units="mV">
      <description/>
    </field>
    <field defaultvalue="IDLE" elements="1" name="State" type="enum" units="">
      <description/>
      <options>
        <option>IDLE</option>
        <option>RESET</option>
        <option>COLLECTING</option>
      </options>
    </field>
  </object>
</xml>
