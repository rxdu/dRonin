{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug_fw_pixracer",
            "type": "cppdbg",
            "request": "launch",
            "miDebuggerPath": "${workspaceRoot}/tools/gcc-arm-none-eabi-6-2017-q2-update/bin/arm-none-eabi-gdb",
            "targetArchitecture": "arm",
            "program": "${workspaceRoot}/build/fw_pixracer/fw_pixracer.elf",
            "setupCommands": [
                { "text": "file ${workspaceRoot}/build/fw_pixracer/fw_pixracer.elf" },
                { "text": "target remote localhost:2331" },
                { "text": "monitor reset" },
                { "text": "load" },
                { "text": "monitor reset"}
            ],
            "preLaunchTask": "make_fw_pixracer",
            "launchCompleteCommand": "None",
            "externalConsole": false,
            "cwd": "${workspaceRoot}"
        }
    ]
}
    